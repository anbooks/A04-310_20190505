@model List<WebApplication8.Models.DbSc>
@{
    Layout = "_Layout2";
}
@{
    ViewData["Title"] = "Create";
}

<h2>详细成绩信息</h2>

<h4>@ViewBag.name</h4>
<hr />

        <form asp-action="Create">
            <table class="table">
                <thead>
                    <tr>


                        <th>
                            一级专业
                        </th>
                        <th>
                            二级专业
                        </th>
                        <th>
                            三级专业
                        </th>
                        <th>
                            分数
                        </th>
                        <th>
                            答题日期
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>


                        <td width="200px">
                            @Html.DisplayFor(modelItem => item.Pa.Sua.SuaName)
                        </td>
                        <td width="200px">
                            @Html.DisplayFor(modelItem => item.Pa.Sub.SubName)
                        </td>
                        <td width="200px">
                            @Html.DisplayFor(modelItem => item.Pa.Suc.SucName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Score)
                        </td>
                        <td width="200px">
                            @Html.DisplayFor(modelItem => item.TestEnd)
                        </td>
                        <td width="200px">
                            <a asp-area="" asp-controller="DbExams" asp-action="Create" asp-route-id="@item.PaId">错题信息</a>
                        </td>
                    </tr>
                    }
                </tbody>
            </table>
        </form>
<a asp-action="Details">返回</a>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
