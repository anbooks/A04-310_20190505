@using HighchartsNETCore;
@model HighchartsNETCore.ChartsSeries;
@{
    ViewData["Title"] = "Grade";
    Layout = "_Layout_Grade";
}
@{
    if (ViewData["ErrorMessage"] != null)
    {
        <script src="~/lib/jquery/dist/jquery.js" type="text/javascript"></script>
        <script type="text/javascript" language="javascript">
            $(document).ready(function () {

                alert("请选择考试名称");

            });
        </script>
    }
}
<h2>部门成绩统计</h2>
<!--<script src="~/js/jquery-2.2.3.min.js"></script>-->
<!--<script src="~/js/highcharts.js"></script>-->

<form asp-action="Grade">
    <div>
        考试名称:<select id="s1" asp-items="ViewBag.branch" name="sel_1">
            <option selected>请选择</option>
        </select>
        <input type="submit" value="查询" />
    </div>

    
</form>
@*<script src="http://cdn.hcharts.cn/jquery/jquery-1.8.3.min.js"></script>
    <script src="http://cdn.hcharts.cn/highcharts/highcharts.js"></script>*@


<script src="~/js/jquery-2.2.3.min.js"></script>
<script src="~/js/highcharts.js"></script>


@if (ViewBag.Series != null)
{
    <div>
        <high-charts id="demoline" title="部门成绩平均分线图" type="Line" series="ViewBag.Series"></high-charts>
        <high-charts id="democolumn" title="部门成绩平均分柱图" type="Column" series="ViewBag.Series"></high-charts>
        <high-charts id="demopie" title="部门成绩平均分饼图" type="Pie" series="ViewBag.Series"></high-charts>
    </div>
}






